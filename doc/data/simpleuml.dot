digraph G {
        fontname = "Bitstream Vera Sans"
        fontsize = 8

        node [
                group = a
                fontname = "Bitstream Vera Sans"
                fontsize = 8
                shape = "record"
        ]


        edge [
                fontname = "Bitstream Vera Sans"
                fontsize = 8
        ]

        Shape [
                label = "{\<\<interface\>\>|Shape}"
        ]
        Circle [
                label = "{Circle}"
        ]
        DecoratedShape [
                label = "{DecoratedShape}"
        ]
        
        AttractShape [
                label = "{AttractShape}"
        ]

        RepelShape [
                label = "{RepelShape}"
        ]


        Circle -> Shape
        DecoratedShape -> Shape
        AttractShape -> DecoratedShape
        RepelShape -> DecoratedShape

        edge [
                arrowhead = "none"

                headlabel = "0..*"
                taillabel = "1"
        ]

}